import java.util.Scanner;

public class ABMINUS2 {
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		int cnt,result =0;
		String input= null;
		cnt = sc.nextInt();
		input =sc.next();
		sc.close();
		
		
		for(int i = 0 ; i<cnt; i++){
			result += Integer.parseInt(input.substring(i, i+1));
		}
		System.out.println(result);
	}
}
