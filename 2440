import java.util.Scanner;

public class ABMINUS2 {
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		int cnt = 0;
		cnt = sc.nextInt();
		
		for(int i = 1 ; i<=cnt ; i++){
			for(int j = cnt ; j>=1; j--){
				if(j<i){
					System.out.print("");
				}else{
					System.out.print("*");
				}
			}
			System.out.println();
		}
	}
}
